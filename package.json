{
  "name": "ecoleta",
  "version": "1.0.0",
  "description": "Aplicação backend desenvolvido em NodeJS para servir como base para a aplicação EColeta",
  "main": "server.js",
  "scripts": {
    "dev": "ts-node-dev --transpileOnly --ignore-watch node_modules src/server.ts",
    "knex:migrate": "knex migrate:latest --knexfile knexfile.ts magrate:latest",
    "knex:seed": "knex --knexfile knexfile.ts seed:run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gisiona/backend-ecoleta.git"
  },
  "keywords": [
    "Ecoleta",
    "NodeJS",
    "Rocketseat"
  ],
  "author": "Gisiona Costa",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Gisiona/backend-ecoleta/issues"
  },
  "homepage": "https://github.com/Gisiona/backend-ecoleta#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "knex": "^0.21.1",
    "sqlite3": "^4.2.0",
    "typescript": "^3.9.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.44"
  }
}
